function TRSMinimap.isResting(): boolean;
var
  tpa: TPointArray;
begin
  result := false;

  // blue color
  if (not findColorsTolerance(tpa, 13345801, self.button[MM_BUTTON_RUN].bounds, 50)) then
    exit();

  result := inRange(length(tpa), 5, 25);
end;


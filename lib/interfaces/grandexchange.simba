function TRSGrandExchange.__getActiveTab(): Integer;
var
  i: Integer;
begin
  for i := 0 to high(self.tabBoxes) do
    if (not (getColor(self.tabBoxes[i].X2, self.tabBoxes[i].Y2) = 9078134)) then
      exit(i);
end;

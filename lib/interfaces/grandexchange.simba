function TRSGrandExchange.__isOfferOpen(waitTime: integer = 500): boolean;
var
  timeOut: UInt64;
  TPA: TPointArray;
begin
  timeOut := getTickCount64() + waitTime;

  while (getTickCount64() < timeOut) do
